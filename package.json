{
  "name": "blog",
  "private": true,
  "version": "2.1.0",
  "description": "A minimal blog, built as an isomorphic JavaScript app powered by WordPress.com",
  "scripts": {
    "dev": "NODE_PATH=src/ node webpack.server.js",
    "build": "NODE_PATH=src/ webpack --config webpack.config.prod.js",
    "postinstall": "if [ \"$NODE_ENV\" = \"production\" ]; then npm run build; fi",
    "start": "NODE_PATH=src/ babel-node src/server.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aduth/blog.git"
  },
  "author": {
    "name": "Andrew Duthie",
    "email": "andrew@andrewduthie.com",
    "url": "https://andrewduthie.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aduth/blog/issues"
  },
  "homepage": "https://github.com/aduth/blog",
  "babel": {
    "stage": 0,
    "optional": [
      "runtime"
    ]
  },
  "engines": {
    "node": "0.12.x"
  },
  "dependencies": {
    "autoprefixer-loader": "^2.0.0",
    "babel": "^5.8.21",
    "babel-core": "^5.8.22",
    "babel-loader": "^5.3.2",
    "babel-runtime": "^5.8.20",
    "body-parser": "^1.13.3",
    "classnames": "^2.1.3",
    "compression": "^1.5.2",
    "css-loader": "^0.16.0",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^0.8.2",
    "flux-standard-action": "^0.6.0",
    "he": "^0.5.0",
    "highlight.js": "^8.7.0",
    "history": "^1.9.1",
    "html-webpack-plugin": "^1.6.1",
    "json-loader": "^0.5.2",
    "lodash": "^3.10.1",
    "moment": "^2.10.6",
    "node-sass": "^3.3.3",
    "raw-loader": "^0.5.1",
    "react": "^0.13.3",
    "react-gravatar": "^2.0.0",
    "react-helmet": "1.1.2",
    "react-highlight": "^0.5.0",
    "react-hot-loader": "^1.2.9",
    "react-redux": "^1.0.1",
    "react-router": "1.0.0-rc1",
    "redux": "^1.0.1",
    "rimraf": "^2.4.3",
    "sass-loader": "^2.0.1",
    "serve-static-cache": "^1.2.0",
    "style-loader": "^0.12.3",
    "superagent": "^1.3.0",
    "superagent-jsonp": "0.0.5",
    "webpack": "^1.12.0",
    "webpack-dev-server": "^1.10.1",
    "wpcom": "4.6.0"
  },
  "devDependencies": {
    "babel-eslint": "^4.1.3",
    "eslint": "^1.5.0",
    "eslint-plugin-react": "^3.4.2"
  }
}
